    <Declaration><![CDATA[{attribute 'qualified_only'}
VAR_GLOBAL
	//################ input variables from HMI ########################
	// control variable for instrument calibration
	g_instruCaliFlag :BOOL:= FALSE;
	g_searchZeroPosFlag :BOOL := FALSE;
	g_writeZeroPosFlag :BOOL :=FALSE;

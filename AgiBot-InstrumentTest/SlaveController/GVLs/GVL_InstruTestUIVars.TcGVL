	// control variable for instrument auto test
	g_instruAutoTestFlag :BOOL :=FALSE;
	g_instruAutoTestMode :INT :=0; // 0: gripper close and open; 1: move with gripper open ; 2: move with max grip force
	g_maxInstruAutoTestCycles :DINT :=0;
	
	// control variable for instrument manual test
	g_instruManualTestFlag :BOOL :=FALSE;
	g_moveToSetPosFlag :BOOL :=FALSE;
	g_targetRollAng :LREAL :=0; //deg
	g_targetPitchAng :LREAL :=-90;//deg

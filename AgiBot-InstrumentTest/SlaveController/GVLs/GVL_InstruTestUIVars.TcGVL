	g_targetGripForceLevel:INT :=0;
	g_gripperOpenFlag :BOOL :=FALSE;
	g_gripperCloseFlag :BOOL :=FALSE;
	g_maxGripForceFlag :BOOL :=FALSE;
	
	// control variable for instrument verify
	g_instruVerifyFlag :BOOL :=FALSE;
	g_instruVerifyLastStepFlag :BOOL:=FALSE;
	g_instruVerifyNextStepFlag :BOOL :=FALSE;
	
	//################ output variables  HMI ########################
	g_curInstruJntPos :Vec5d;
	g_curInstruJntCurrent:Vec5d;
	g_curInstruDHPos :Vec3d;
	g_curGripAng:LREAL;
	g_instruZeroPos :Vec5d;
	g_instruSN :ULINT;

END_VAR]]></Declaration>
